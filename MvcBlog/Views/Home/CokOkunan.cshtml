@model IEnumerable<MvcBlog.Models.Makale>

@{ 
    Layout = null;
}
<h5 class="card-header">En çok okunan makaleler</h5>
<div class="row">
    <div class="col-lg-12">
        <ul>
            @foreach (var item in Model)
            {
                <li><a href="/Home/MakaleDetay/@item.Makaleid">@item.Baslik</a>(@item.Okunma)</li>
            }
        </ul>

    </div>
</div>

